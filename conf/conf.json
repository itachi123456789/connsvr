{
    "dev": {
        "app": {
            "name": "connsvr",
            "tport": 44391,
            "hport": 44392,
            "bport": 44393
        },
        "cons": {
            "buf_size": 128, // 一次读取数据大小, 大于大部分数据包长
            "buf_size4http": 128, // 一次读取数据大小, 大于http包头第一行
            "body_len_limit": 4096, // 包最大长度
            "u_map_num": 300, // 用户分组hash
            "room_msg_len": 10000, // 房间消息队列长度
            "c_rtimeout": "2m", // 读超时
            "c_wtimeout": "10ms", // 写超时
            "c_rbuf": 512, // 读缓冲区大小
            "c_wbuf": 2048 // 写缓冲区大小
        },
        "varhost": { // 定时获取远程服务器最新配置，用于参数动态变更
